<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/padding_auto_complete_item">

    <ImageView
        android:id="@+id/iv_auto_complete_item"
        android:layout_width="@dimen/width_iv_auto_complete_item"
        android:layout_height="match_parent"
        android:src="@drawable/ic_menu_gallery"
        />
    <TextView
        android:id="@+id/tv_text_auto_complete"
        android:paddingLeft="@dimen/padding_auto_complete_item"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

</LinearLayout>